{
  "name": "verified-react",
  "sources": [
    {
      "dir": "examples",
      "subdirs": true,
      "generators": [{
        "name": "imandra-extract-re",
        "edge": ["tictactoe/TicTacToeLogic.ml", ":", "tictactoe/TicTacToeLogic.ire"]
      },{
        "name": "imandra-extract-re",
        "edge": ["simple/SimpleModel.ml", ":", "simple/SimpleModel.ire"]
      }]
    },
    {
      "dir": "scripts"
    },
    {
      "dir": "src"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "reason-react",
    "bs-imandra-client",
    "@glennsl/bs-jest",
    "@glennsl/bs-json",
    "imandra-prelude",
    "bs-css"
  ],
  "reason": {
    "react-jsx": 2
  },
  "warnings": {
    "error" : "+101"
  },
  "refmt": 3,
  "generators": [
    {
      "name": "imandra-extract-re",
      "command": "imandra-extract -reason -o $out $in"
    },
    {
      "name": "imandra-extract-ml",
      "command": "imandra-extract -o $out $in"
    }
  ]
}
